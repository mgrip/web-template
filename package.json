  "scripts": {
    "dev": "NODE_ENV=development npx concurrently \"npx webpack --watch\" \"npx webpack serve --hot\" \"npx wait-on dist/server.js && npx nodemon dist/server.js\"",
    "codegen": "graphql-codegen --config codegen.yml",
    "build": "npx webpack && npx prisma generate && npx prisma migrate deploy",
    "start": "node dist/server.js",
    "build:cypress": "NODE_ENV=test npx prisma generate && npx prisma migrate reset -f",
    "start:cypress": "NODE_ENV=test npx concurrently \"npx webpack\" \"npx webpack serve --hot\" \"npx wait-on dist/server.js && node dist/server.js\""
